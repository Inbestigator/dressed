{
  "name": "dressed",
  "private": true,
  "workspaces": [
    "packages/*",
    "www"
  ],
  "scripts": {
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "checks": "turbo dist && bun eslint && bun format && turbo test",
    "outdated": "for pkg in packages/*; do (cd \"$pkg\" && bun outdated); done"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/bun": "1.2.18",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "packageManager": "bun@1.2.13"
}
