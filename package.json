{
  "name": "dressed",
  "private": true,
  "workspaces": [
    "packages/*",
    "www"
  ],
  "scripts": {
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "checks": "turbo dist && bun --bun biome check --write && turbo test",
    "outdated": "for pkg in packages/*; do (cd \"$pkg\" && bun outdated); done"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "packageManager": "bun@1.2.13",
  "dependencies": {
    "@types/bun": "^1.2.21"
  }
}
