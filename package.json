{
  "name": "dressed",
  "private": true,
  "workspaces": [
    "packages/*",
    "www"
  ],
  "scripts": {
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "checks": "turbo dist && bun eslint && bun format && turbo test",
    "outdated": "for pkg in packages/*; do (cd \"$pkg\" && bun outdated); done"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "packageManager": "bun@1.2.13",
  "dependencies": {
    "@types/bun": "^1.2.21"
  }
}
