  },
  "exports": {
    ".": "./dist/index.js",
    "./server": "./dist/server/index.js",
    "./utils": "./dist/utils/index.js"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "homepage": "https://dressed.js.org",
  "keywords": [
    "bot",
    "serverless",
    "discord",
    "discord-bot"
  ],
  "license": "MIT",
